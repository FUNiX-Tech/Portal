<odoo>
    <data noupdate="0">

        <!-- Định nghĩa tăng Id cho project -->
        <record id="seq_mentor_project" model="ir.sequence">
            <field name="name">Project ID</field>
            <field name="code">learning_project.project</field>
            <field name="prefix">A</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

        <!-- Định nghĩa tăng Id cho Submission -->
        <record id="seq_mentor_submission" model="ir.sequence">
            <field name="name">Submission ID</field>
            <field name="code">learning_project.project_submission</field>
            <field name="prefix">S</field>
            <field name="padding">3</field>
            <field name="company_id" eval="False"/>
        </record>

    </data>
</odoo>
