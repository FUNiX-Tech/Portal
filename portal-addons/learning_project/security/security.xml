<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_category_project" model="ir.module.category">
        <field name="name">Project Category Module</field>
        <field name="description">Project Category Module</field>
    </record>

    <record id="group_project_admin" model="res.groups">
        <field name="name">Project Admin</field>
        <field name="category_id" ref="module_category_project"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
        <field name="comment">Project Admin</field>
    </record>
</odoo>
