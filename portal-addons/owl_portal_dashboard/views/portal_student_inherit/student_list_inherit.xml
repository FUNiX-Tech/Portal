<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="owl_portal_student_list_view_inherit" model="ir.ui.view">
        <field name="name">owl_portal_student_list_view_inherit</field>
        <field name="model">portal.student</field>
        <field name="inherit_id" ref="portal_student_management.student_tree"></field>
        <field name="arch" type="xml">
            <xpath expr="//tree" position="attributes">
                <attribute name="js_class">owl_student_list_view</attribute>
            </xpath>
        </field>
    </record>


</odoo>