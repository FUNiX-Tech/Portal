<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="module_category_assignment" model="ir.module.category">
        <field name="name">Assignment Category Module</field> 
        <field name="description">Assignment Category Module</field> 
    </record>

    <record id="group_assignment_admin" model="res.groups">
        <field name="name">Assignment Admin</field>
        <field name="category_id" ref="module_category_assignment"/> 
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>  
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/> 
        <field name="comment">Assignment Admin</field> 
    </record>
</odoo>