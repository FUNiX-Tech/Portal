<odoo>
    <data>
        <record id="view_student_organization_form_inherit" model="ir.ui.view">
            <field name="name">student_organization.form.inherit.po_learning_program</field>
            <field name="model">student_organization</field>
            <field name="inherit_id" ref="student_organization.student_organization_form" />
            <field name="arch" type="xml">
                <xpath expr="//form/sheet/notebook" position="inside">
                        <page string="PO Learning Program">
                            <field name="purchased_po_learning_program_ids" options="{'no_create': False}" />
                        </page>

                        <page string="Course Access">
                            <field name="course_access_ids" options="{'no_create': False}" />
                        </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
