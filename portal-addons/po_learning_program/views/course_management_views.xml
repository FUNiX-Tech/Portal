<odoo>
    <data>
        <record id="view_course_management_form_inherit" model="ir.ui.view">
            <field name="name">course_management.form.inherit.po_learning_program</field>
            <field name="model">course_management</field>
            <field name="inherit_id" ref="course_management.view_course_management_form" />
            <field name="arch" type="xml">
                <xpath expr="//form/sheet" position="inside">
                    <notebook>
                        <page string="Organizations">
                            <field name="organization_ids" options="{'no_create': False}" />
                        </page>

                        <page string="Individual Student Access">
                            <field name="individual_student_ids" options="{'no_create': False}" />
                        </page>

                        <page string="PO Learning Program">
                            <field name="po_learning_program_ids" options="{'no_create': False}" />
                        </page>
                    </notebook>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
